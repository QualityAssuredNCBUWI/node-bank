{
  "name": "node-bank2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "unit-test": "./node_modules/karma/bin/karma start karma.conf.js",
    "load-test": "artillery run ./test/load/test.yml",
    "load-test-report": "artillery run --output ./test/load/report.json ./test/load/test.yml && artillery report --output ./test/load/report.html ./test/load/report.json",
    "e2e-test": "./node_modules/.bin/cypress open",
    "unit-test-windows": "karma start karma.conf.js",
    "load-test-windows": "artillery run ./test/load/test.yml",
    "load-test-report-windows": "artillery run --output ./test/load/report.json ./test/load/test.yml && artillery report --output ./test/load/report.html ./test/load/report.json",
    "e2e-test-windows": "cypress open"
  },
  "author": "Jeffrey Nerona",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "^4.17.1",
    "express-redirect": "^1.2.2",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "artillery": "^1.6.0-14",
    "cypress": "^6.8.0",
    "jasmine-core": "^2.2.0",
    "karma": "^6.1.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^0.3.8",
    "karma-ng-html2js-preprocessor": "^0.1.0",
    "karma-safari-launcher": "^1.0.0",
    "nodemon": "^1.17.2"
  }
}
